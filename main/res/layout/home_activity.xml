<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainBackground"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="au.net.nicksifniotis.amedatest.HomeActivity">


    <ImageButton
        android:background="@drawable/h_btn_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/h_btn_fore_tutorial"
        android:scaleType="centerInside"
        android:padding="20dp"
        android:id="@id/h_btn_tutorial"
        android:layout_above="@id/h_btn_familiarisation"
        android:onClick="h_btn_tutorial"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:background="@drawable/h_btn_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/h_btn_fore_familiarisation"
        android:scaleType="centerInside"
        android:padding="20dp"
        android:id="@id/h_btn_familiarisation"
        android:onClick="h_btn_familiarisation"
        android:layout_centerInParent="true" />

    <ImageButton
        android:background="@drawable/h_btn_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/h_btn_fore_begintest"
        android:scaleType="centerInside"
        android:padding="20dp"
        android:id="@id/h_btn_begintest"
        android:onClick="h_btn_begintest"
        android:layout_below="@id/h_btn_familiarisation"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
